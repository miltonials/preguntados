@model IEnumerable<preguntados.Models.Vhistorialresultado>

@{
    ViewData["Title"] = "Index";
}

<h1>Ranking de jugadores</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Jugador)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Aciertos)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Jugador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Aciertos)
            </td>
        </tr>
}
    </tbody>
</table>
